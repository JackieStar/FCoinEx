page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.container {
  padding-top: 100px;
  position: relative;
  overflow: hidden;
  background: url(/static/images/public/bg.png);
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
}
.wrapper {
  position: relative;
  z-index: 90;
  padding-bottom: 40rpx;
}
.back-btn {
  position: absolute;
  left: 40rpx;
  z-index: 9999;
  padding-top: 25px;
  top: 40rpx;
  font-size: 40rpx;
  color: #ffffff;
}
.left-top-sign {
  font-size: 120rpx;
  color: #070219;
  position: relative;
  left: -16rpx;
}
.right-top-sign {
  position: absolute;
  top: 80rpx;
  right: -30rpx;
  z-index: 95;
}
.right-top-sign:before, .right-top-sign:after {
  display: block;
  content: '';
  width: 400rpx;
  height: 80rpx;
  background: #b4f3e2;
}
.right-top-sign:before {
  -webkit-transform: rotate(50deg);
          transform: rotate(50deg);
  border-radius: 0 50px 0 0;
}
.right-top-sign:after {
  position: absolute;
  right: -198rpx;
  top: 0;
  -webkit-transform: rotate(-50deg);
          transform: rotate(-50deg);
  border-radius: 50px 0 0 0;
  /* background: pink; */
}
.left-bottom-sign {
  position: absolute;
  left: -270rpx;
  bottom: -320rpx;
  border: 100rpx solid #d0d1fd;
  border-radius: 50%;
  padding: 180rpx;
}
.welcome {
  position: relative;
  padding-left: 40rpx;
  padding-bottom: 50rpx;
}
.welcome .logo {
  width: 150rpx;
}
.welcome .txt {
  display: flex;
  flex-direction: column;
  color: #ffffff;
  padding-left: 20rpx;
  padding-bottom: 40rpx;
  font-size: 26rpx;
}
.welcome .txt .b {
  font-size: 48rpx;
  font-weight: bold;
}
.welcome .txt .tips {
  font-size: 24rpx;
  font-weight: 300;
  color: #ffffff;
  opacity: 0.5;
  margin-top: 23rpx;
}
.input-content {
  padding: 0 60rpx;
}
.input-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding: 0 10rpx;
  height: 80rpx;
  margin-bottom: 50rpx;
  border-bottom: 1px solid #0B2771;
  color: #ffffff;
}
.input-item:last-child {
  margin-bottom: 0;
}
.input-item .icon {
  width: 13px;
  height: 17px;
}
.input-item .tit {
  height: 50rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  color: #606266;
}
.input-item input {
  height: 60rpx;
  font-size: 30rpx;
  color: #ffffff;
  width: 100%;
  padding-left: 20rpx;
}
.link {
  display: flex;
  padding: 30rpx 60rpx;
  color: #ffffff;
}
.confirm-btn {
  width: 630rpx;
  height: 76rpx;
  line-height: 76rpx;
  margin-top: 40rpx;
  margin: 40rpx auto;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #fff;
  font-size: 34rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.forget-section {
  font-size: 26rpx;
  color: #4399fc;
  text-align: center;
  margin-top: 40rpx;
}
.register-section {
  position: absolute;
  left: 0;
  bottom: 50rpx;
  width: 100%;
  font-size: 26rpx;
  color: #4e46d2;
  text-align: center;
}
.register-section text {
  color: #ffffff;
  margin-left: 10rpx;
}
.code-btn {
  float: right;
  width: 200rpx;
  text-align: right;
  color: #4F5B87;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  background: linear-gradient(0deg, #3FBBFE 0%, #A541FF 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

